program ugly_search;
const
	ZERO = 0;
	MAX_LEN = 100;
type
	Array = array[ZERO .. MAX_LEN] of integer;
var 
	index : integer;
function search(
var	
	arr : Array;
size, element : integer // size表示数组大小
) : boolean; // 返回数组是否包含元素element
begin
	index := 0;
1:
	if arr[index] = element then
	begin
		search := true;
	end;
	if index >= MAX_LEN then
	begin
		search := false;
	end;
	index := index + 1;
	goto 1;
end
