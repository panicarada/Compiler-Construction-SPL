program test_record;


type 
	int = integer;
	Info = record 	
			 Age 	: int;		
			 Name   : string; 	
			 GPA    : real;
		  end;
var
	info : Info;


procedure wrap(
	Age : int;
	Name : string;
	GPA  : real;
var
	info : Info
);
begin
	info.Age := Age;
	info.Name := Name;
	info.GPA := GPA;
end
;

begin
	wrap(8, "QZH", 5.555, info);
	writeln("Name: ", info.Name, "	age: ", info.Age, "	GPA: ", info.GPA);
end
.
