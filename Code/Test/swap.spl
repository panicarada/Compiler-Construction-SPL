program swap;
var
	x : real;
	y : real;
	temp : real;
procedure swap(
	temp : real;
var 
	x, y : real
);
begin
	temp := x;
	x := y;
	y := temp;
end;

begin
	read("Please enter your x: ", x);
	read("Please enter your y: ", y);
	swap(temp, x, y);
	writeln("After Swap:");
	writeln("x: ", x, "	y: ", y);
end
.